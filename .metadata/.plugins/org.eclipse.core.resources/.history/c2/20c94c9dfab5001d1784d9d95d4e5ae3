	public static void bangCuuChuong() {
	    for (int i = 2; i < 10; i++) {
	        System.out.println("Bảng cửu chương " + i);
	        for (int j = 1; j < 11; j++) {
	            System.out.println(i + " x " + j + " = " + i*j);
	        }
	        System.out.println();
	    }
	}

}
