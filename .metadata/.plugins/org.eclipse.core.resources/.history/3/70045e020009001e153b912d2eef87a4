
public class Cau1 {
	public static int [][] MuoiDiem (int arr[]){
		int arr2[][] = new int [arr.length][arr.length];
		for(int i = 0; i < arr.length - 1; i++) {
			arr2[i][arr.length - i]= arr[arr.length-i];
		}
	
		return null;
	}
	
	public static void main(String[] args) {
		int arr[] = {9, 5, 3, 10, 8, 20};
	}
}
